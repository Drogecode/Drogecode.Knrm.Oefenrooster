@if (_series is not null)
{
    <div>
        <AuthorizeView Roles="@AccessesNames.AUTH_Taco">
            @_elapsedMilliseconds
        </AuthorizeView>
        <SelectMultipleUsers Users="@Users" Functions="@Functions" Selection="@_selectedUsersAction" SelectionChanged="OnSelectionChanged" MultiSelection="true"/>
        <MudChart ChartType="ChartType.Line" ChartSeries="@_series" XAxisLabels="@_xAxisLabels" Width="100%" Height="350px" ChartOptions="@_options" CanHideSeries/>
    </div>
}
else
{
    <MudSkeleton/>
    <MudSkeleton/>
    <MudSkeleton/>
    <MudSkeleton/>
}