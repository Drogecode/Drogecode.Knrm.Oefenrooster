@if (_years is not null)
{
    <MudSelect T="int" Label="@L["year"]" SelectedValues="_selectedYear" SelectedValuesChanged="SelectedYearChanged" Clearable="false" MultiSelection="false">
        @foreach (var year in _years)
        {
            <MudSelectItem T="int" Value="@year">@year</MudSelectItem>
        }
    </MudSelect>
    <MudPaper>
        Komt vast nog wel een keer.
    </MudPaper>
}
else
{
    <DrogeLoading />
}