{
  "openapi": "3.0.1",
  "info": {
    "title": "PreCom",
    "version": "v1"
  },
  "paths": {
    "/api/PreCom/web-hook/{customerId}/{id}": {
      "post": {
        "tags": [
          "PreCom"
        ],
        "operationId": "WebHook",
        "parameters": [
          {
            "name": "customerId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "sendToHub",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": true
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { }
            },
            "text/json": {
              "schema": { }
            },
            "application/*+json": {
              "schema": { }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/PreCom/{take}/{skip}": {
      "get": {
        "tags": [
          "PreCom"
        ],
        "operationId": "AllAlerts",
        "parameters": [
          {
            "name": "take",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "skip",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/MultiplePreComAlertsResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MultiplePreComAlertsResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/MultiplePreComAlertsResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "MultiplePreComAlertsResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "offline": {
            "type": "boolean"
          },
          "elapsedMilliseconds": {
            "type": "integer",
            "format": "int64"
          },
          "totalCount": {
            "type": "integer",
            "format": "int32"
          },
          "preComAlerts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PreComAlert"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PreComAlert": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "notificationId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "alert": {
            "type": "string",
            "nullable": true
          },
          "sendTime": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "priority": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "rawJson": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}