{
  "openapi": "3.0.1",
  "info": {
    "title": "Authentication",
    "version": "v1"
  },
  "paths": {
    "/api/Authentication/login-secrets": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "operationId": "GetLoginSecrets",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/Authentication/authenticat-user": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "operationId": "AuthenticatUser",
        "parameters": [
          {
            "name": "code",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "state",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sessionState",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "redirectUrl",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "boolean"
                }
              },
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              },
              "text/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    },
    "/api/Authentication/current-user-info": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "operationId": "CurrentUserInfo",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              }
            }
          }
        }
      }
    },
    "/api/Authentication/logout": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "operationId": "Logout",
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Authentication/refresh": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "operationId": "Refresh",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "boolean"
                }
              },
              "application/json": {
                "schema": {
                  "type": "boolean"
                }
              },
              "text/json": {
                "schema": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CurrentUser": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "isAuthenticated": {
            "type": "boolean"
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "claims": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/StringStringKeyValuePair"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GetLoginSecretsResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "offline": {
            "type": "boolean"
          },
          "elapsedMilliseconds": {
            "type": "integer",
            "format": "int64"
          },
          "loginSecret": {
            "type": "string",
            "nullable": true
          },
          "loginNonce": {
            "type": "string",
            "nullable": true
          },
          "codeChallenge": {
            "type": "string",
            "nullable": true
          },
          "tenantId": {
            "type": "string",
            "format": "uuid"
          },
          "clientId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "StringStringKeyValuePair": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "nullable": true
          },
          "value": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}