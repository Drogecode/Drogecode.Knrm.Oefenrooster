{
  "openapi": "3.0.1",
  "info": {
    "title": "Authentication",
    "version": "v1"
  },
  "paths": {
    "/api/Authentication/login-secrets": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "operationId": "GetLoginSecrets",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetLoginSecretsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/Authentication/login-callback": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "operationId": "Logincallback",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "id_token": {
                    "type": "string"
                  },
                  "state": {
                    "type": "string"
                  },
                  "session_state": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "id_token": {
                  "style": "form"
                },
                "state": {
                  "style": "form"
                },
                "session_state": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Authentication/current-user-info": {
      "get": {
        "tags": [
          "Authentication"
        ],
        "operationId": "CurrentUserInfo",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/CurrentUser"
                }
              }
            }
          }
        }
      }
    },
    "/api/Authentication": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "operationId": "Logout",
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CurrentUser": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "isAuthenticated": {
            "type": "boolean"
          },
          "userName": {
            "type": "string",
            "nullable": true
          },
          "claims": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/StringStringKeyValuePair"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "GetLoginSecretsResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "offline": {
            "type": "boolean"
          },
          "elapsedMilliseconds": {
            "type": "integer",
            "format": "int64"
          },
          "loginSecret": {
            "type": "string",
            "nullable": true
          },
          "loginNonce": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "StringStringKeyValuePair": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "nullable": true
          },
          "value": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}